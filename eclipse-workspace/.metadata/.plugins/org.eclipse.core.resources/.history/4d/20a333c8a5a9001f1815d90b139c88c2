package com.springcore.ConstructorInjection;

public class Person {
	private String name;
	private int personId;
	
	public Person(String name, int personId) {
		this.name = name;
		this.personId = personId;
	}
	
	@Override
	public String toString() {
		return "Name : " + this.name + "\nPersonId : " + this.personId;
	}
}
